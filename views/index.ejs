<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <script src="/socket.io/socket.io.js"></script>
  </head>
  <script>
    var socket = io('ws://localhost:8888/');
    socket.on('connect', function () {
      socket.send('hi');

      socket.on('message', function (msg) {
        // my msg
        alert(msg);
      });
    });
  </script>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
  </body>
</html>